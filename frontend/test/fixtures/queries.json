                "courses_dept"
            ],
            "APPLY": [
                {
                    "countAudit": {
                        "COUNT": "courses_audit"
                    }
                },
                {
                    "maxFail": {
                        "MAX": "courses_fail"
                    }
                }
            ]
        }
    },
    "query7": {
        "WHERE": {
            "IS": {
                "rooms_fullname": "M*"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_address",
