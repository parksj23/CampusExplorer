{
    "query1": {
        "WHERE": {
            "IS": {
                "courses_dept": "cpsc"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id"
            ],
            "ORDER": "courses_id"
        }
    },
    "query2": {
        "WHERE": {
            "AND": [
                {
                    "NOT": {
                        "GT": {
                            "courses_avg": 70
                        }
                    }
                },
                {
                    "IS": {
                        "courses_dept": "engl"
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_id",
                "countDept",
                "countYear"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "countDept"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept",
                "courses_id",
                "courses_year"
            ],
            "APPLY": [
                {
                    "countDept": {
                        "COUNT": "courses_dept"
                    }
                },
                {
                    "countYear": {
                        "COUNT": "courses_year"
                    }
                }
            ]
        }
    },
    "query3": {
        "WHERE": {
            "IS": {
                "courses_dept": "crwr"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_avg",
                "courses_id",
                "maxAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "courses_avg"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_avg",
                "courses_id"
            ],
            "APPLY": [
                {
                    "maxAvg": {
                        "MAX": "courses_avg"
                    }
                }
            ]
        }
    },
    "query4": {
        "WHERE": {
            "EQ": {
                "courses_pass": 1
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "maxAudit",
                "avgFail",
                "avgYear"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "avgFail"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "maxAudit": {
                        "MAX": "courses_audit"
                    }
                },
                {
                    "avgFail": {
                        "AVG": "courses_fail"
                    }
                },
                {
                    "avgYear": {
                        "AVG": "courses_year"
                    }
                }
            ]
        }
    },
    "query5": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats",
                "rooms_furniture"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname",
                "rooms_furniture"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "query6": {
        "WHERE": {
            "GT": {
                "courses_audit": 20
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_avg",
