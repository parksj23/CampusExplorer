{
    "title": "Valid -- Rooms example with added column",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats",
                "rooms_furniture"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname",
                "rooms_furniture"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_shortname": "OSBO",
            "maxSeats": 442,
            "rooms_furniture": "Classroom-Movable Tables & Chairs"
        },
        {
            "rooms_shortname": "HEBB",
            "maxSeats": 375,
            "rooms_furniture": "Classroom-Fixed Tables/Fixed Chairs"
        },
        {
            "rooms_shortname": "LSC",
            "maxSeats": 350,
            "rooms_furniture": "Classroom-Fixed Tables/Movable Chairs"
        }
    ]
}
