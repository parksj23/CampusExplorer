{
    "title": "Valid -- Rooms SELECT Room Addr -- *Input",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_address": "*Mall"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_fullname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_fullname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
