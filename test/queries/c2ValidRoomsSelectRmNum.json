{
    "title": "Valid -- Rooms SELECT Room Number",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_number": "2"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 500
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_shortname": "WOOD",
            "maxSeats": 503
        }
    ]
}
