    "title": "c2 valid -- Simple SELECT Instructor",
    "query": {
        "WHERE": {
            "EQ": {
                "courses_pass": 0
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "avgFail"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "avgFail"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "avgFail": {
                        "AVG": "courses_fail"
                    }
                }
            ]
        }
    },
