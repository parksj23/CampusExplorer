{
    "title": "Valid -- Simple SELECT rooms_address",
    "query": {
        "WHERE": {
            "IS": {
                "rooms_address": "2357 Main Mall"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_fullname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "maxSeats",
                    "rooms_fullname"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_fullname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_fullname": "MacMillan",
            "maxSeats": 200
        }
    ]
}
