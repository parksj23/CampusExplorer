{
    "title": "Valid -- Simple SELECT rooms_number",
    "query": {
        "WHERE": {
            "IS": {
                "rooms_number": "100"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_fullname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "maxSeats",
                    "rooms_fullname"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_fullname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
