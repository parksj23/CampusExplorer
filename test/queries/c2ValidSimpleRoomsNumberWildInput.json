    "title": "Valid -- Simple SELECT rooms_number -- *input",
    "query": {
        "WHERE": {
            "IS": {
                "rooms_number": "*20"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_fullname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "maxSeats",
                    "rooms_fullname"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_fullname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
