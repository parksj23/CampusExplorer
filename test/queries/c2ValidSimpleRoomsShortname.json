{
    "title": "Valid -- Simple SELECT rooms_shortname",
    "query": {
        "WHERE": {
            "IS": {
                "rooms_shortname": "ALRD"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_fullname",
                "avgSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "avgSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_fullname"
            ],
            "APPLY": [
                {
                    "avgSeats": {
                        "AVG": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_fullname": "Allard Hall (LAW)",
            "avgSeats": 45.6
        }
